/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=not_a);
    Not(in=b, out=not_b);
    And(a=a, b=not_b, out=and_not_b);
    And(a=not_a, b=b, out=and_not_a);
    Or(a=and_not_b, b=and_not_a, out=out);
}
